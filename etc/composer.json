{
	"name": "rnd.feide.no",
	"description": "This is a technology blog at UNINETT AS",
	"version": "1.0.0",
	"type": "root",
	"keywords": [],
	"homepage": "http://rnd.feide.no",
	"minimum-stability": "stable",
	"repositories": [{
		"type": "composer",
		"url": "https://wpackagist.org"
	}],
	"require": {
		"composer/installers": "~1.0",
		"johnpbloch/wordpress": "~4.6",
		"johnpbloch/wordpress-core-installer": "~0.2",
		"wpackagist-plugin/wordpress-seo": "~3.3",
		"wpackagist-plugin/dataporten-oauth": "~2.5",
		"wpackagist-plugin/w3-total-cache": "~0.9.4.1",
		"wpackagist-plugin/enlighter": "~3.1",
		"wpackagist-plugin/jetpack": "~4.0",
		"wpackagist-plugin/google-analytics-for-wordpress": "~5.5",
		"wpackagist-plugin/convert-post-types": "~1.4",
		"wpackagist-plugin/list-category-posts": "~0.68",
		"wpackagist-plugin/post-type-switcher": "~2.0.1",
		"wpackagist-plugin/embed-github-gist": "0.13",
		"wpackagist-plugin/simple-custom-post-type-archives": "~0.9.3",
