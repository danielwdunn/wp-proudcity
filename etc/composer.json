	"_requireDisabled": {
		"wpackagist-plugin/disable-xml-rpc": "~1.0.1",
		"wpackagist-plugin/google-sitemap-generator": "~4.0.8",
		"wpackagist-plugin/wptouch": "~4.1.4",
		"wpackagist-plugin/twitter-for-wordpress": "~1.9.7",
		"wpackagist-plugin/wp-to-twitter": "~3.2.10",
		"wpackagist-plugin/recently-registered": "~3.4.3",
		"wpackagist-plugin/ghost": "~0.5.6"
